{
    "extends": [],
    "rules": {
        "max-line-length": {
            "options": [
                180
            ]
        },
        "indent": [
            true,
            "spaces",
            4
        ],
        "new-parens": true,
        "arrow-parens": false,
        "no-arg": true,
        "no-var-keyword": true,
        "prefer-const": true,
        "quotemark": [
            false
        ],
        "curly": false,
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-bitwise": true,
        "array-type": false,
        "no-empty": false,
        "interface-name": false,
        "no-trailing-whitespace": false,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}